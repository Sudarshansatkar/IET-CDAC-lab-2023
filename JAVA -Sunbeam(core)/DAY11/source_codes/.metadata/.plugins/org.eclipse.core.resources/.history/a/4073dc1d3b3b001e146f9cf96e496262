import java.util.ArrayList;


public class Day11_13 {

	public static void main(String[] args) 
	{
		ArrayList<Integer> l1 = new ArrayList<Integer>();
		l1.add(11);
		l1.add(22);
		l1.add(33);
		l1.add(44);
		l1.add(55);
		l1.add(9.4);
		l1.add("sunbeam");
	}
}



