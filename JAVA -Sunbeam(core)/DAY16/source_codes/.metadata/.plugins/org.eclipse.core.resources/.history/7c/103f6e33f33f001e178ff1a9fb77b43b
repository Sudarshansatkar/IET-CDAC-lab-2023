import java.util.ArrayList;



public class Day16_9 
{
	@SafeVarargs
	static void disp(int... a)
	{
		for(int element : a)
			System.out.print(element +"\t");
		System.out.println();
	}
	
	
	
	public static void main(String[] args) 
	{
		disp(10);
		disp(10,20);
		disp(10,20,30);
		
	}
}




