// reference inside the class
class Date
{
	private int dd;
	private int mm;
	private int yy;
	public int getDd() {
		return dd;
	}
	public void setDd(int dd) {
		this.dd = dd;
	}
	public int getMm() {
		return mm;
	}
	public void setMm(int mm) {
		this.mm = mm;
	}
	public int getYy() {
		return yy;
	}
	public void setYy(int yy) {
		this.yy = yy;
	}
	
	void disp()
	{
		System.out.println("DD = "+this.dd+" MM = "+this.mm+" YY = "+this.yy);
	}
	
}
class Employee
{
	int empid; // p type ==> 0
	Date dobj; // NP type ==> reference ==> null
	
}
