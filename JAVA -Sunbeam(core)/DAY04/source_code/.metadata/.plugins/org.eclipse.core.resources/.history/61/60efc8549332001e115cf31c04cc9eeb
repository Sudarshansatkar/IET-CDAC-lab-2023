class Employee
{
	private String name;
	private int empid;
	private float salary;
	private int deptid;
	
	public Employee() 
	{
		this.name="DEFAULT";
		this.empid=1;
		this.salary=50000;
		this.deptid=1;
		
	}

	public Employee(String name, int empid, float salary, int deptid) {
		
		this.name = name;
		this.empid = empid;
		this.salary = salary;
		this.deptid = deptid;
	}
	
	
	
	
	
	
}


