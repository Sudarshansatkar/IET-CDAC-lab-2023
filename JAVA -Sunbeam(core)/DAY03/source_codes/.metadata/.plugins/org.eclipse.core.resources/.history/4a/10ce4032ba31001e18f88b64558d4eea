
//Boxing
public class Day3_4 
{

	public static void main(String[] args) 
	{
		int num1=35;
		float fval = 7.84f;
		double dval = 9.8;
		Integer i1 = Integer.valueOf(num1);
		// num1 ==> P type
		// i1 ==> NP type
		// P to NP // BOXING 
		// int to Integer ===> valueOf() 
		
		Float f1 = Float.valueOf(fval);
		Double d1 = Double.valueOf(dval);
	}
}





