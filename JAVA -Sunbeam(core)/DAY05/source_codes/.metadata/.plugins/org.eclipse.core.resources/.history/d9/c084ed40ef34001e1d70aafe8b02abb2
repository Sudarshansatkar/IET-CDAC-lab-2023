
public class Day5_9 {

	public static void main(String[] args) 
	{
		System.out.println(Math.abs(3.5));
		System.out.println(Math.ceil(9.4));
		System.out.println(Math.floor(9.4));
		System.out.println(Math.max(85,55));
		System.out.println(Math.min(35,51));
		System.out.println(Math.sqrt(4));
		System.out.println(Math.pow(6, 2));
		

	}

}
