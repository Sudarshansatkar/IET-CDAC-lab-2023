// Method reference to an instance method of an object


@FunctionalInterface
interface MyInterface
{
	void display();
}


class Mobile
{
	
}





public class Day18_10 
{

	 	 
	public static void main(String[] args) 
	{
		Day18_10 obj = new Day18_10();
		MyInterface ref = obj::m1;
		ref.display();
	}

}
