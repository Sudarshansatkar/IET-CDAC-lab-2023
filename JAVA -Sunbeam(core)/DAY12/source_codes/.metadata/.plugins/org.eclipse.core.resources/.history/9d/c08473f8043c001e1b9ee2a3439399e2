class product
{
	int pid;
	float price;
	
	public product() 
	{
		super();
	}

	public product(int pid, float price) {
		super();
		this.pid = pid;
		this.price = price;
	}

	@Override
	public String toString() 
	{
		return "product [pid=" + pid + ", price=" + price + "]";
	}
	
}
public class Day12_14 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
