import java.util.Scanner;

public class Day6_4 {

	
	void accept(int arr[])
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Please enter array elements : ");
		for(int i=0;i<arr.length;i++)
			arr[i] = sc.nextInt();
	}
	public static void main(String[] args) 
	{
		int a[]=null;
		accept(a);
		disp(a);
		
	}

}
