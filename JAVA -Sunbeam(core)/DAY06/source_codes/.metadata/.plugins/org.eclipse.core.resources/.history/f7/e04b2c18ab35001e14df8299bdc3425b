import java.util.Arrays;

// Methods of Arrays class


public class Day6_8 {

	public static void main(String[] args) 
	{
		int a1[]= {11,22,33,44,55};
		System.out.println("Search element 33 = "+Arrays.binarySearch(a1, 33));
	}
}




