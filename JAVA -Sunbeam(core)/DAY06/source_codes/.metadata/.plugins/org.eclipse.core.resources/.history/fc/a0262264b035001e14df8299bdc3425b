// Array of References

class Employee
{
	private String name;
	private int id;
	private float salary;
	
	public Employee()
	{
		
	}

	public Employee(String name, int id, float salary) 
	{
		this.name = name;
		this.id = id;
		this.salary = salary;
	}
	
	
	
	
}
