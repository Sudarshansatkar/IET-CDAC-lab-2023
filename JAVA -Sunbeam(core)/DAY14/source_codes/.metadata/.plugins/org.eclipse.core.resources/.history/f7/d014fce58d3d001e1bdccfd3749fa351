import java.util.HashMap;
import java.util.LinkedHashMap;
import java.util.TreeMap;

//TreeMap

//UnOrdered, Sorted, no duplicates are allowed, 
//if we try to add same key with different value , value of that key gets overwritten
//value can be null for more than one key
//Key should be unique
//one null key and one null value record is allowed
//one null key can be allocated with one value also 

public class Day14_5 {

	public static void main(String[] args) {
		TreeMap<Integer,String> tm = new TreeMap<Integer,String>();
		tm.put(60, "p1");
		tm.put(20, "p2");
		tm.put(30, "p3");
		tm.put(40,"p4");
		tm.put(40, "p4"); // duplicate not allowed 
		tm.put(40, "p5");
		tm.put(50, null);
		tm.put(80, null);
		//tm.put(null, "n1");
		
		System.out.println("Linked Hash map contents = "+tm);
		
	}

}




