import java.util.HashMap;

//HashMap
// Unordered, Unsorted, no duplicates are allowed, 
// if we try to add same key with different value , value of that key gets overwritten
// value can be null for more than one key
// Key should be unique
// one null key and one null value record is allowed

public class Day14_5 {

	public static void main(String[] args) {
		HashMap<Integer,String> hm = new HashMap<Integer,String>();
		hm.put(10, "p1");
		hm.put(20, "p2");
		hm.put(30, "p3");
		hm.put(40,"p4");
		hm.put(40, "p4"); // duplicate not allowed 
		hm.put(40, null);
		hm.put(50, null);
		hm.put(null, null);
		hm.put(null, "n1");
		
		System.out.println("Hash map contents = "+hm);
		
	}

}
