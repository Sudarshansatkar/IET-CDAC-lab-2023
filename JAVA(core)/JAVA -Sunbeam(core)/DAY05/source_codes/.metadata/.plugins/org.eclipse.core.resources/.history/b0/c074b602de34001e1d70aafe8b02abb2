package p1;

import java.util.Scanner;

// final keyword ==> constant

//final field
class Demo
{
	final float PI=3.14f;  // final field 
	int num1; //non final field
	int num2; //non final field 
	
	Demo()
	{
		this.num1=5;
		this.num2=5;
	}
	
}










