package p1;

public class Circle extends BoundedShape
{
	private int radius;
	
	public Circle(int x,int y,int radius)
	{
		super(x,y);
		this.radius=radius;
	}

	@Override
	public String toString() {
		return "Circle [radius=" + radius + "]"+ super.toString();
	}

	@Override
	public double area() 
	{
		return Math.PI * radius * radius;
	}
	
	
	
	
	
	
}
