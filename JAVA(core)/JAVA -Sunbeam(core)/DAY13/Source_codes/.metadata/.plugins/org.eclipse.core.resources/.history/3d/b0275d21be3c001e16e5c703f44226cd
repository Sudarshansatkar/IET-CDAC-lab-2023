import java.util.ArrayList;
import java.util.Vector;

// Example of Vector class



public class Day13_4
{

	public static void main(String[] args) {
		ArrayList<Integer> a1 = new ArrayList<Integer>(3);
		a1.add(10);
		a1.add(20);
		a1.add(30);
		System.out.println("A1 = "+a1);
		a1.add(40);
		System.out.println("A1 = "+a1);
	}
}


