//take input from user using command line


class Day2_8
{ 
    public static void main(String args[])
    {
        int n1 = Integer.parseInt(args[0]); //String to int 
        int n2 = Integer.parseInt(args[1]);
        System.out.println("N1 = "+n1+" N2 = "+n2+ " Result = "+(n1+n2));
        
    }

}