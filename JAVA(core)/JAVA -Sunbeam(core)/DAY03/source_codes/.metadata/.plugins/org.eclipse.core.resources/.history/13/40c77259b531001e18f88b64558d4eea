
public class Day3_2 {

	public static void main(String[] args) 
	{
		int num = 60; // num ==> p type ==> int
		double d1 = num;  // d1 ==> p type ==> double
		// int to double // p to p conversion
		//small to large // WIDENING
		
	}
}




