
public class Day7_4 {

	public static void main(String[] args)
	{
		String s1 = new String("Hello"); // NL 
		System.out.println("S1 = "+s1);
		System.out.println("S1 = "+(s1.concat("Everyone")));
		System.out.println("S1 = "+s1 ); //immutable
		s1 = s1.concat("Everyone");
		System.out.println("S1 = "+s1 ); 
	}

}
