
//String and its methods

public class Day7_4 {

	public static void main(String[] args)
	{
		String str = "Sunbeam";
		String str1 = "Sunbeam";
		String str2 = "sunbeam";
		char ch = str.charAt(1);
		System.out.println("Character at 1st location = "+ch);
		System.out.println("Compare to = "+str.compareTo(str1));
		System.out.println("Compare to = "+str1.compareTo(str2));
		//Compares two strings lexicographically.
		// S = 83   // s = 115  
		// 83 - 115 = -32 
		
		
		
	}
}


