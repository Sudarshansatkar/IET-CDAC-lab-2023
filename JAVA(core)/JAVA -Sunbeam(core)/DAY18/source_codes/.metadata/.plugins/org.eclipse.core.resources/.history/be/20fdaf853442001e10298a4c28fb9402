interface Shape
{
	float PI =3.14f;
	void area();
	default void disp()
	{
		System.out.println("PI = "+PI);
		System.out.println("inside disp of default method");
	}
	
}

class Rectangle implements Shape
{

	@Override
	public void area() 
	{
		
		
	}
	
}

class Circle implements Shape
{
	
}

