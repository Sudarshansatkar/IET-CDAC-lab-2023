import java.util.Arrays;
import java.util.List;

//multi line lambda expressions


public class Day18_5 {

	public static void main(String[] args) 
	{
		List<Integer> l1 = Arrays.asList(10,20,30,40);
		
	}
}




// Single line lambda expression
