import java.util.ArrayList;




public class Day11_13 {

	public static void main(String[] args) 
	{
		ArrayList<Integer> l1 = new ArrayList<Integer>();
		l1.add(11);
		l1.add(22);
		l1.add(33);
		l1.add(44);
		l1.add(55); 
		System.out.println("L1 = "+l1);
		System.out.println("Fetch the element at location 1 = "+l1.get(1));
		System.out.println("Index of element 44 = "+l1.indexOf(44));
		System.out.println("Index of element 99 = "+l1.indexOf(99));
	}
}


