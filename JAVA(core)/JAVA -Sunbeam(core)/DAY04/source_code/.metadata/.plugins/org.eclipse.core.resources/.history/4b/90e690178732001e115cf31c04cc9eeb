class Student
{
	private int rollno;
	private String grade;
	
	// getters
	int getRollNo()
	{
		return this.rollno;
	}
	
	String getGrade()
	{
		return this.grade;
	}
	
	//setters
	
	void setRollNo(int rollno)
	{
		this.rollno=rollno;
	}
	
	void setGrade(String grade)
	{
		this.grade   = grade;
	}
	
	//facilitator
	void disp()
	{
		System.out.println("Rollno = "+this.rollno+ " Grade = "+this.grade);
	}
}
public class Day4_7 {

	public static void main(String[] args) 
	{
		Student s1 = new Student();

	}

}
