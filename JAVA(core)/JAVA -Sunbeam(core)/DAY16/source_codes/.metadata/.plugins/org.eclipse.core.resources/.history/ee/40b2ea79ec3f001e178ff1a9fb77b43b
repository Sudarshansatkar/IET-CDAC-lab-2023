class Person
{
	void disp()
	{
		System.out.println("inside person class disp method");
	}
}
class Employee extends Person
{
	
	void m1()  // non overridden method
	{
		System.out.println("inside method m1");
	}
	
	//the target for using the annotation @Override is a method
		
	@Override
	void disp() // overridden method
	{
	
		System.out.println("inside employee class disp method");
	}
}
public class Day16_9 {

	public static void main(String[] args) 
	{
		
	}

}
